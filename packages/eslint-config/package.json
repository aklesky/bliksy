{
    "name": "@aklesky/eslint-config",
    "version": "0.2.0",
    "description": "Shareable collection of eslint configurations.",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "commonjs",
    "main": "dist/index.js",
    "prettier": "@aklesky/prettier-config",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./node.js": {
            "require": "./dist/node.js",
            "import": "./dist/node.js"
        },
        "./jest": {
            "require": "./dist/jest.js",
            "import": "./dist/jest.js"
        },
        "./jest.js": {
            "require": "./dist/jest.js",
            "import": "./dist/jest.js"
        },
        "./package.json": "./package.json"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "npm run clean && tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/ts-config": "0.2.0",
        "@types/eslint": "8.21.0",
        "prettier": "2.8.3",
        "typescript": "4.9.5"
    },
    "dependencies": {
        "@aklesky/prettier-config": "0.2.0",
        "@typescript-eslint/eslint-plugin": "5.51.0",
        "@typescript-eslint/parser": "5.51.0",
        "eslint": "8.33.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "rimraf": "4.1.2"
    }
}
