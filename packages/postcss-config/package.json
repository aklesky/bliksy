{
    "name": "@aklesky/postcss-config",
    "version": "0.6.1",
    "description": "Postcss config presets",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "prettier": "@aklesky/prettier-config",
    "main": "dist/index.js",
    "exports": {
        "./*": "./dist/*"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "lint": "eslint --ext .ts ./src",
        "build": "npm run clean && tsc --project ./tsconfig.build.json",
        "test": "jest --passWithNoTests"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "dependencies": {
        "autoprefixer": "10.4.13",
        "cssnano": "5.1.14",
        "postcss": "8.4.21",
        "postcss-load-config": "4.0.1",
        "postcss-nesting": "11.1.0",
        "rimraf": "4.1.2"
    },
    "devDependencies": {
        "@aklesky/eslint-config": "0.6.0",
        "@aklesky/prettier-config": "0.6.0",
        "@aklesky/ts-config": "0.6.0"
    }
}
