    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "dependencies": {
        "autoprefixer": "10.4.16",
        "cssnano": "6.0.2",
        "postcss": "8.4.32",
        "postcss-load-config": "5.0.2",
        "postcss-nesting": "12.0.1"
    },
    "devDependencies": {
        "@aklesky/eslint-config": "0.7.14",
        "@aklesky/prettier-config": "0.7.14",
        "@aklesky/ts-config": "0.7.14"
    }
