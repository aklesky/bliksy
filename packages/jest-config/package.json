{
    "name": "@aklesky/jest-config",
    "version": "0.0.10",
    "description": "Shareable jest configuration.",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/bliksy#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/@types/index.d.ts",
    "type": "commonjs",
    "prettier": "@aklesky/prettier-config",
    "directories": {
        "dist": "dist"
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "types": "./@types/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./config.js": {
            "types": "./@types/config.d.ts",
            "require": "./dist/config.js",
            "import": "./dist/config.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/bliksy.git"
    },
    "scripts": {
        "clean": "rm -rf ./dist && rm -rf ./@types",
        "build": "npm run clean && tsc --project tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/bliksy/issues"
    },
    "devDependencies": {
        "@aklesky/eslint-config": "^0.0.10",
        "@aklesky/prettier-config": "^0.0.9",
        "@aklesky/ts-config": "^0.0.9",
        "@types/jest": "29.2.3",
        "jest": "29.3.1",
        "typescript": "4.9.5"
    },
    "dependencies": {
        "@swc/core": "1.3.19",
        "@swc/jest": "0.2.23"
    }
}
