{
    "name": "@aklesky/prettier-config",
    "version": "0.0.9",
    "description": "Shareable prettier configuration.",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/bliksy#readme",
    "license": "MIT",
    "type": "commonjs",
    "main": "dist/index.js",
    "types": "@types/index.d.ts",
    "prettier": "./dist/config.js",
    "exports": {
        ".": {
            "types": "./@types/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./config.js": {
            "types": "./@types/config.d.ts",
            "require": "./dist/config.js",
            "import": "./dist/config.js"
        },
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/bliksy.git"
    },
    "scripts": {
        "clean": "rm -rf ./dist && rm -rf ./@types",
        "build": "npm run clean && tsc --project tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/bliksy/issues"
    },
    "devDependencies": {
        "@aklesky/ts-config": "^0.0.9",
        "@types/prettier": "2.7.1",
        "prettier": "2.7.1",
        "typescript": "4.9.3"
    }
}
