{
    "name": "@aklesky/streamable-react",
    "version": "0.6.2",
    "description": "React Server Side Streamable Rendering",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "prettier": "@aklesky/prettier-config",
    "type": "module",
    "exports": {
        "./*": "./dist/*"
    },
    "keywords": [
        "react",
        "ssr",
        "server",
        "middleware",
        "stream",
        "rendering"
    ],
    "main": "lib/react-server-middleware.js",
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "lint": "eslint --ext .ts,.tsx ./src",
        "build": "npm run clean && tsc --project ./tsconfig.build.json",
        "test": "jest"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/eslint-config": "0.6.2",
        "@aklesky/jest-config": "0.6.2",
        "@aklesky/prettier-config": "0.6.2",
        "@aklesky/ts-config": "0.6.2",
        "@types/express": "4.17.17",
        "@types/node": "18.16.18",
        "@types/react": "18.2.13",
        "@types/react-dom": "18.2.6",
        "@types/supertest": "2.0.12",
        "express": "4.18.2",
        "supertest": "6.3.3"
    },
    "dependencies": {
        "@aklesky/utilities": "0.6.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "4.4.0"
    }
}
