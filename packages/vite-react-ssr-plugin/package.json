{
    "name": "@aklesky/vite-react-ssr-plugin",
    "version": "0.6.2",
    "description": "Vite Plugin to handle react server side rendering.",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "module",
    "prettier": "@aklesky/prettier-config",
    "main": "dist/index.js",
    "exports": {
        "./*": "./dist/*"
    },
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "lint": "eslint --ext .ts ./src",
        "clean": "rimraf -rf ./dist",
        "build": "npm run clean && tsc --project ./tsconfig.build.json",
        "test": "jest --passWithNoTests"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/eslint-config": "0.6.2",
        "@aklesky/prettier-config": "0.6.2",
        "@aklesky/ts-config": "0.6.2",
        "vite": "4.1.1"
    },
    "dependencies": {
        "@aklesky/streamable-react": "0.6.2",
        "@aklesky/utilities": "0.6.2",
        "rimraf": "4.1.2"
    }
}
