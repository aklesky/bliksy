{
    "name": "@aklesky/eslint-config",
    "version": "0.6.7",
    "description": "Shareable collection of eslint configurations.",
    "author": "Aleksey Semiglasov <aleksey.semiglasov@gmail.com>",
    "homepage": "https://github.com/aklesky/node-workspace#readme",
    "license": "MIT",
    "type": "commonjs",
    "main": "dist/index.js",
    "prettier": "@aklesky/prettier-config",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./node": {
            "require": "./dist/node.js",
            "import": "./dist/node.js"
        },
        "./node.js": {
            "require": "./dist/node.js",
            "import": "./dist/node.js"
        },
        "./jest": {
            "require": "./dist/jest.js",
            "import": "./dist/jest.js"
        },
        "./jest.js": {
            "require": "./dist/jest.js",
            "import": "./dist/jest.js"
        },
        "./package.json": "./package.json"
    },
    "directories": {
        "dist": "dist",
        "@types": "@types"
    },
    "files": [
        "dist",
        "@types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aklesky/node-workspace.git"
    },
    "scripts": {
        "clean": "rimraf -rf ./dist",
        "build": "npm run clean && tsc --project ./tsconfig.build.json"
    },
    "bugs": {
        "url": "https://github.com/aklesky/node-workspace/issues"
    },
    "devDependencies": {
        "@aklesky/ts-config": "0.6.7",
        "prettier": "3.0.2",
        "rimraf": "5.0.1",
        "typescript": "5.1.6"
    },
    "dependencies": {
        "@aklesky/prettier-config": "0.6.7",
        "@types/eslint": "8.44.2",
        "@typescript-eslint/eslint-plugin": "6.7.2",
        "@typescript-eslint/parser": "6.7.2",
        "eslint": "8.47.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-jest": "27.2.3",
        "eslint-plugin-prettier": "5.0.0"
    }
}
